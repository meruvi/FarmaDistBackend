<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cofar.backendapolo.pruebas.mapper.CursoMapper">
    <select id="listar" resultType="com.cofar.backendapolo.pruebas.model.Curso">
        select c.ID_CURSO as idCurso, c.NOMBRE_CURSO as nombreCurso
        from curso c
    </select>
</mapper>
