<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cofar.backendapolo.pruebas.mapper.ClienteMapper">
    <select id="listarClientes" resultType="com.cofar.backendapolo.pruebas.model.Cliente">
        select c.cod_cliente as codCliente, c.nombre_cliente as nombreCliente
        from clientes c
    </select>
</mapper>
